rcount = len(r)

def r_value(n):

    if n < rcount:
        return r[n]

    # Even n
    if n & 1 == 0:
        m = n >> 1
        value = r_value(m) + r_value(m + 1) + m
        return value

    # Odd n
    if n & 1 == 1:
        m = n >> 1
        value = r_value(m - 1) + r_value(m) + 1
        return value

def generate_values(limit):
    for i in range(limit):
        v = r_value(i)
        it = (i, v)
        print it
        if (i > limit):
            break

if __name__ == '__main__':
    generate_values(10000)
